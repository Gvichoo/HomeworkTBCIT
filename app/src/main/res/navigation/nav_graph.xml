<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/mainFragment">

    <fragment
        android:id="@+id/mainFragment"
        android:name="com.example.homeworktbc.fragmentMain.MainFragment"
        android:label="fragment_main"
        tools:layout="@layout/fragment_main" >
        <action
            android:id="@+id/action_mainFragment_to_successfulFragment"
            app:destination="@id/successfulFragment" />
    </fragment>
    <fragment
        android:id="@+id/successfulFragment"
        android:name="com.example.homeworktbc.fragmentSuccessful.SuccessfulFragment"
        android:label="fragment_successful"
        tools:layout="@layout/fragment_successful" >
        <action
            android:id="@+id/action_successfulFragment_to_mainFragment"
            app:destination="@id/mainFragment" />
    </fragment>
</navigation>